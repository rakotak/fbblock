{
  "manifest_version": 3,
  "name": "Hide Facebook Ads & Reels",
  "version": "1.0",
  "description": "Hides Facebook ads and reels",
  "icons": {
    "48": "icons/icon48.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["content.js"]
    }
  ]
}
